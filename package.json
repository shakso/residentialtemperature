{
  "name": "admin-dashboard",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"vite\" \"npm run services\" --kill-others-on-fail --handle-input --names \"vite,services\" --prefix-colors \"blue.bold,green.bold\"",
    "services": "concurrently \"npm run stripe-server\" \"npm run email-server\" \"npm run report-scheduler\" --kill-others-on-fail --handle-input --names \"stripe,email,reports\" --prefix-colors \"magenta.bold,cyan.bold,yellow.bold\"",
    "stripe-server": "tsx watch src/server/stripe-api.ts",
    "email-server": "tsx watch src/server/email-api.ts",
    "report-scheduler": "NODE_ENV=development NODE_OPTIONS=\"--max-old-space-size=128\" tsx watch src/server/report-scheduler.ts",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "pm2 start ecosystem.config.js --no-daemon"
  },
  "dependencies": {
    "concurrently": "^8.2.2",
    "tsx": "^4.7.1",
    "node-fetch": "^3.3.2",
    "@stripe/react-stripe-js": "^2.5.0",
    "@stripe/stripe-js": "^3.0.5",
    "aws-sdk": "^2.1574.0",
    "@supabase/supabase-js": "^2.39.8",
    "pg": "^8.11.3",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "apexcharts": "^4.0.0",
    "@hcaptcha/react-hcaptcha": "^1.9.3",
    "chart.js": "^4.4.2",
    "cors": "^2.8.5",
    "date-fns": "^3.3.1",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "jsdom": "^24.0.0",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.2",
    "lucide-react": "^0.344.0",
    "pm2": "^5.3.1",
    "react": "^18.3.1",
    "react-apexcharts": "^1.4.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.22.3",
    "@sendgrid/mail": "^7.7.0",
    "node-fetch": "^3.3.2",
    "stripe": "^14.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/cors": "^2.8.17",
    "@types/jsdom": "^21.1.6",
    "@types/pg": "^8.11.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/express": "^4.17.21",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^4.5.2",
    "@tailwindcss/forms": "^0.5.7"
  }
}
